{
    "version": 2,
    "builds": [
        {
            "src": "public/*",
            "use": "@now/static"
        },
        {
            "src": "parser.js",
            "use": "@now/node"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "/public/index.html"
        },
        {
            "src": "/(.+)",
            "dest": "/parser.js",
            "headers": {
                "Access-Control-Allow-Origin": "*"
            }
        }
    ],
    "name": "now-mercury-parser"
}
